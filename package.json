{
  "name": "ngx-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run watch",
    "watch": "ng serve --hmr --aot",
    "watch:jit": "ng serve --hmr",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "lint": "ng lint",
    "doc": "compodoc -p src/tsconfig.app.json --silent --output docs",
    "compile:production": "ng build --prod",
    "build:production": "run-s lint test:coverage compile:production doc",
    "serve:production": "ws --port 8080  --compress --directory dist/ngx-portal --spa index.html",
    "stats:production": "webpack-bundle-analyzer dist/ngx-portal/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0-beta.5",
    "@angular/common": "~7.0.0-beta.5",
    "@angular/compiler": "~7.0.0-beta.5",
    "@angular/core": "~7.0.0-beta.5",
    "@angular/forms": "~7.0.0-beta.5",
    "@angular/http": "~7.0.0-beta.5",
    "@angular/platform-browser": "~7.0.0-beta.5",
    "@angular/platform-browser-dynamic": "~7.0.0-beta.5",
    "@angular/pwa": "^0.9.0-beta.3",
    "@angular/router": "~7.0.0-beta.5",
    "@angular/service-worker": "~7.0.0-beta.5",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.9.0-beta.1",
    "@angular/cli": "~7.0.0-beta.3",
    "@angular/compiler-cli": "~7.0.0-beta.5",
    "@angular/language-service": "~7.0.0-beta.5",
    "@compodoc/compodoc": "^1.1.5",
    "@types/node": "^10.9.4",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.0",
    "local-web-server": "^2.6.0",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^23.10.0-beta.6",
    "tslint": "~5.11.0",
    "tslint-angular": "^1.1.2",
    "typescript": "~3.0.1",
    "webpack-bundle-analyzer": "^3.0.2"
  }
}
